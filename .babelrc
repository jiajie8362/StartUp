{
  "presets": ["es2015"],
  "plugins": [
    "transform-function-bind",
    "transform-class-properties",
    ["transform-es2015-classes", { "loose": true }],
    "transform-object-rest-spread",
    "transform-async-to-generator",
    "transform-export-extensions",
    "add-module-exports",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ],
  "comments": false
}
